#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/xeptore/dev/semver-prase/node_modules/.pnpm/is-ci@3.0.0/node_modules/is-ci/node_modules:/home/xeptore/dev/semver-prase/node_modules/.pnpm/is-ci@3.0.0/node_modules:/home/xeptore/dev/semver-prase/node_modules/.pnpm/node_modules:/home/xeptore/dev/semver-prase/node_modules:/home/xeptore/dev/node_modules:/home/xeptore/node_modules:/home/node_modules:/node_modules:/home/xeptore/dev/semver-prase/node_modules/.pnpm/jest-config@27.0.6/node_modules/jest-config/node_modules:/home/xeptore/dev/semver-prase/node_modules/.pnpm/jest-config@27.0.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/xeptore/dev/semver-prase/node_modules/.pnpm/is-ci@3.0.0/node_modules/is-ci/node_modules:/home/xeptore/dev/semver-prase/node_modules/.pnpm/is-ci@3.0.0/node_modules:/home/xeptore/dev/semver-prase/node_modules/.pnpm/node_modules:/home/xeptore/dev/semver-prase/node_modules:/home/xeptore/dev/node_modules:/home/xeptore/node_modules:/home/node_modules:/node_modules:/home/xeptore/dev/semver-prase/node_modules/.pnpm/jest-config@27.0.6/node_modules/jest-config/node_modules:/home/xeptore/dev/semver-prase/node_modules/.pnpm/jest-config@27.0.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-ci@3.0.0/node_modules/is-ci/bin.js" "$@"
else
  exec node  "$basedir/../../../../../is-ci@3.0.0/node_modules/is-ci/bin.js" "$@"
fi
